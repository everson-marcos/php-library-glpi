{
    "version": "0.1.1",
    "name": "everson-marcos/php-library-glpi",
    "type": "library",
    "description": "GLPI API Client Library for PHP",
    "keywords": [
        "GLPI",
        "rest",
        "api",
        "client"
    ],
    "homepage": "https:\/\/glpi-project.github.io\/php-library-glpi\/",
    "license": "MIT",
    "authors": [
        {
            "name": "Everson Marcos de Souza",
            "email": "e.ever.marcos@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Thierry Bugier",
            "email": "tbugier@teclib.com",
            "role": "Developer"
        },
        {
            "name": "Domingo Oropeza",
            "email": "doropeza@teclib.com",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "contact@teclib.com",
        "irc": "irc:\/\/irc.freenode.org\/glpi"
    },
    "require": {
        "php": ">=7.0",
        "guzzlehttp\/guzzle": "^7.2"
    },
    "require-dev": {
        "atoum\/atoum": "^3.1",
        "atoum\/stubs": "^2.5",
        "atoum\/json-schema-extension": "^2.1",
        "glpi-project\/tools": "^0.1.2",
        "atoum\/reports-extension": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Glpi\\Api\\": "src\/Glpi\/Api\/"
        },
        "exclude-from-classmap": [
            "tests\/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Glpi\\Api\\Rest\\tests\\": "tests\/"
        }
    },
    "config": {
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": false
}
